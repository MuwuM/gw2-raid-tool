<div class="arc-list">
  <nav>
    <ul class="pagination justify-content-center">
      <li class="page-item <%=(page <= 0)?'disabled':'' %>"><a class="page-link" href="?p=0">«</a></li>
      <li class="page-item <%=(page <= 0)?'disabled':'' %>"><a class="page-link" href="?p=<%= Math.max(page-1,0) %>">‹</a></li>
      <li class="page-item disabled w-50 text-center"><a class="page-link"><%= (page+1) %>/<%= (maxPages) %></a></li>
      <li class="page-item <%=(page >= maxPages-1)?'disabled':'' %>"><a class="page-link" href="?p=<%= Math.min(maxPages-1,page+1) %>">›</a></li>
      <li class="page-item <%=(page >= maxPages-1)?'disabled':'' %>"><a class="page-link" href="?p=<%= (maxPages-1) %>">»</a></li>
    </ul>
  </nav>
  <div class="list-group">
    <% for(const log of logs){ %>
    <a
      href="/log/<%=log.hash%>"
      target="arc-log-display-iframe"
      rel="noreferrer noopener"
      class="arc-list-row list-group-item list-group-item-action d-flex justify-content-between align-items-center <%= (log===logs[0])?'bg-info':'' %>"
      onclick="Array.from(document.querySelectorAll('.arc-list .list-group .list-group-item')).map(i=>i.classList.remove('bg-info'));this.classList.add('bg-info');this.blur()"
    >
    <% if(log.displayCollapse){ %>
      <div class="arc-list-img-box arc-list-img-box-collapse"><%= log.displayCollapse %></div>
      <div class="arc-list-img-bossname">
        <div>
          <span class="arc-list-img-result badge <%=(log.success)?'badge-success':'badge-danger'%>" data-rel-time="<%= log.timeEndMs %>"><%= log.timeEndDiff %></span>
        </div>
      </div>
      <% if(log.displayNameCollapse){%>
        <span class="arc-list-img-logby badge badge-primary badge-pill"><%= log.recordedBy %></span>
      <%} %>
    <% } else { %>
      <div class="arc-list-img-box">
        <%if(log.fightIcon){%>
        <img
          class="arc-list-preview-icon"
          src="<%= log.fightIcon %>"
          alt=""
          rel="noreferrer noopener"
        >
        <%}%>
      </div>
      <div class="arc-list-img-bossname">
        <div><%= log.fightName %></div>
        <div>
          <span class="arc-list-img-result badge <%=(log.success)?'badge-success':'badge-danger'%>" data-rel-time="<%= log.timeEndMs %>"><%= log.timeEndDiff %></span>
        </div>
      </div>
      <span class="arc-list-img-logby badge badge-primary badge-pill"><%= log.recordedBy %></span>
    <%} %>
  </a>
  <% } %>
  <nav>
    <ul class="pagination justify-content-center">
      <li class="page-item <%=(page <= 0)?'disabled':'' %>"><a class="page-link" href="?p=0">«</a></li>
      <li class="page-item <%=(page <= 0)?'disabled':'' %>"><a class="page-link" href="?p=<%= Math.max(page-1,0) %>">‹</a></li>
      <li class="page-item disabled w-50 text-center"><a class="page-link"><%= (page+1) %>/<%= (maxPages) %></a></li>
      <li class="page-item <%=(page >= maxPages-1)?'disabled':'' %>"><a class="page-link" href="?p=<%= Math.min(maxPages-1,page+1) %>">›</a></li>
      <li class="page-item <%=(page >= maxPages-1)?'disabled':'' %>"><a class="page-link" href="?p=<%= (maxPages-1) %>">»</a></li>
    </ul>
  </nav>
</div>
</div>