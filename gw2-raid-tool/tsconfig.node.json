{
  "extends": "@electron-toolkit/tsconfig/tsconfig.node.json",
  "include": [
    "electron.vite.config.*",
    "src/main/**/*",
    "src/preload/*",
    "src/i18n/*",
    "src/info/*",
    "src/raid-tool.ts",
    "src/main/util/load-config.js"
  ],
  "compilerOptions": {
    "composite": true,
    "types": [
      "electron-vite/node"
    ],
    "noImplicitAny": true
  }
}
